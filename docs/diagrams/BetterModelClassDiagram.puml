@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam backgroundColor GRAY
skinparam classBackgroundColor MODEL_COLOR
skinparam classFontColor black

class "HashSet<String>" as DisplaySuperGroups
class "HashSet<String>" as DisplaySubGroups
HashSet *- String
Notor *-right-> "1" UniquePersonList
Notor *-right-> "1" UniqueSuperGroupList
Notor *-right-> "1" Trie
Trie -[hidden]down- UniquePersonList
Trie -[hidden]down- UniquePersonList

Trie *-right-> "*" String
UniquePersonList -right-> Person
UniqueSuperGroupList -right-> SuperGroup
SuperGroup  *--> "*" SubGroup

Group <|-- SuperGroup
Group <|-- SubGroup

Group  *--> "*" Person
Group *--> Note

Person -up-> "*" String

Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Note
Person *--> "*" DisplaySuperGroups
Person *--> "*" DisplaySubGroups

@enduml
